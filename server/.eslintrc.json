{
  "parser": "babel-eslint",
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier/babel", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "ignorePatterns": ["node_modules/", "build/"],
  "rules": {
    "no-console": "off"
  }
}
